FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "900";
    object      mu;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0.001;

boundaryField
{
    left
    {
        type            calculated;
        value           uniform 0.001;
    }
    right
    {
        type            calculated;
        value           uniform 0.001;
    }
    bottom
    {
        type            calculated;
        value           uniform 0.001;
    }
    top
    {
        type            calculated;
        value           uniform 0.001;
    }
    back
    {
        type            empty;
    }
    front
    {
        type            empty;
    }
}


