FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     rhoDarcyFoam;

startFrom       startTime;

startTime       0.0;

stopAt          endTime;

endTime         5000;//3110400000;//622080000; //20 years 1 yr. = 31104000

deltaT          1;//876600; // 10.15 days

writeControl    adjustableRunTime;

writeInterval   100;//31104e3; //1 year = 31104e3;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

adjustTimeStep  yes;

runTimeModifiable yes;

maxDeltaT	100;//2592e3; //1 month
maxCo           1.;
// ************************************************************************* //
