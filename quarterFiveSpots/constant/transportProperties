FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

mu             mu [1 -1 -1 0 0 0 0]    1e-3;
rho            rho [1 -3 0 0 0 0 0]    1;
Ss             Ss   [-1 1 2 0 0 0 0]   0.0;
Dm             Dm   [0 2 -1 0 0 0 0]   1e-5;
beta           beta [0 0 0 0 0 0 0]    0.1;
tau            tau [-1 1 1 0 0 0 0]    1e-5;

hydrostaticPressure on;
concentrationBased off;
dispersionModel off;
incompressible on;
standardCompressibility on;

densityModel
{
    type linearDensity;
    rho0  1000.;
    beta  200.;
}

viscosityModel
{
  type constantViscosity;
  mu0  1e-3;
}

// ************************************************************************* //
