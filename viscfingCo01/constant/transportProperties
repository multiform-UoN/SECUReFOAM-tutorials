FoamFile
{
  version     2.0;
  format      ascii;
  class       dictionary;
  location    "constant";
  object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel  Newtonian;
Ss             Ss   [-1 1 2 0 0 0 0]   0.0;
Dm             DT   [0 2 -1 0 0 0 0]   1e-3;
beta           beta [0 0 0 0 0 0 0]    1.;
hydrostaticPressure on;
concentrationBased off;
dispersionModel off;
incompressible on;

densityModel
{
  type constantDensity;
  rho0 1.;
}

viscosityModel
{
  type exponentialViscosity;
  mu0  1.;
  R   -3.;
}
// ************************************************************************* //
