FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{

    left
    {
        type            zeoGradient;
        value           uniform (0 0 0);
    }

    right
    {
        type            zeroGradient;
        value           uniform (0 0 0);
    }

    top
    {
        type            zeroGradient;
        value           uniform (0 0 0);
    }

    bottom
    {
        type            zeroGradient;
        value           uniform (0 0 0);
    }

    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }

}


